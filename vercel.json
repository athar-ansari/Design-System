{
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        }
    ],
    "builds": [
        {
            "src": "storybook-static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/"
        }
    ],
    "outputDirectory": "storybook-static"
}