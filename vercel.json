{
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "builds": [
        {
            "src": "storybook-static/**",
            "use": "@vercel/static"
        }
    ],
    "outputDirectory": "storybook-static"
}